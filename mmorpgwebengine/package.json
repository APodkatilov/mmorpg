  "description": "MMORPG web engine.",
  "main": "index.jsx",
  "private": true,
  "scripts": {
    "heroku": "npm run server:build && npm run testbox:build:dev && npm run testbox:deploy:dist && npm run server:run:dev",
    "start": "concurrently \"npm:server:debug:dev\" \"npm:testbox:start:dev\"",
    "server:clean": "rimraf ./dist",
    "server:build": "rimraf dist/ && rimraf server/public && babel ./server --out-dir dist/ --copy-files",
    "server:run:dev": "cross-env NODE_ENV=development node dist",
    "server:run:prod": "cross-env NODE_ENV=production node dist",
    "server:debug:dev": "cross-env NODE_ENV=development nodemon --watch server --exec babel-node server/index.js",
    "testbox:clean": "rimraf ./build/testbox",
    "testbox:start:dev": "cross-env NODE_ENV=development AREA=testbox SERVER_PORT=3030 node clientdev",
    "testbox:build:dev": "cross-env NODE_ENV=development webpack --config internals/webpack/webpack.dev.babel.js --env.area=testbox  --color -d --progress --hide-modules --display-optimization-bailout",
    "testbox:build:prod": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.dev.babel.js --env.area=testbox  --color -p --progress --hide-modules --display-optimization-bailout",
    "testbox:deploy:server": "copyfiles  -u 1 build/**/* server/public",
    "testbox:deploy:dist": "copyfiles -u 1 build/**/* dist/public",
    "prod-db-seed": "cross-env DB_SEED=true NODE_ENV=development MONGODB_URL=\"mongodb+srv://admin:admin@mmorpgcuslter-s85rv.mongodb.net/mmorpg?retryWrites=true&w=majority\" babel-node server/index.js",
    "db-seed": "cross-env DB_SEED=true NODE_ENV=development  babel-node server/index.js",
    "eslintinit": "eslint --init",
    "lint-server": "eslint ./server",
    "lint-testbox": "eslint ./testbox",
    "mocha": "mocha --require @babel/register 'test/**/*.js'",
    "jest": "cross-env NODE_ENV=test jest --coverage",
    "generate": "plop --plopfile internals/generators/index.js"
  },
